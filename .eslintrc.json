{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 15,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": 0,
    "no-param-reassign": 0,
    "no-console": 0,
    "max-classes-per-file": 0
  }
}
